		sum(tiisg931.role.40):role.40.total,
		sum(tiisg931.role.41):role.41.total,
		sum(tiisg931.role.42):role.42.total,
		sum(tiisg931.role.43):role.43.total,
		sum(tiisg931.role.44):role.44.total,
